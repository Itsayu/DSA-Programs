#include<iostream>
#include<stack>
using namespace std;

class Stack{
    public:
    int *arr;
    int top;
    int size;
    Stack(int size){
        this->size = size;
        arr = new int[size];
        top = -1;
    }
    void push(int element){
         if(size - top > 1){
             top++;
             arr[top] = element;
         }
         else{
             cout<<"Stack Overflow"<<endl;
         }
    }
    void pop(){
        if(top >= 0){
            top--;
        }
        else{
            cout<<"Stack underflow"<<endl;
        }
    }
    int peek(){
        if(top >= 0 && top < size){
            return arr[top];
        }
        else{
            cout<<"Stack is Empty"<<endl;
            return -1;
        }
    }
    bool isEmpty(){
        if(top==-1){
            return true;
        }
        else{
            return false;
        }
    }
};

int main(){
    Stack nr(6);
    nr.push(1);
    nr.push(22);
    nr.push(9);
    nr.push(5);
    nr.push(7);
    nr.push(11); 
    int n=nr.size;
    for(int i=0; i<n; i++){
        int akt = nr.peek();
        nr.pop();
        cout<<akt<<endl;
    }
}
