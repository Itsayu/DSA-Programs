#include<iostream>
using namespace std;

bool search(int arr[], int size, int key){
    for(int i=0; i<size; i++){
        if(arr[i]==key){
            return true;
        }
    }
    return false;
}

int main(){
    int arr[11]={4,2,5,1,8,6,9,7,0};
    cout<<"Enter the element you want to search: "<<endl;
    int key;
    cin>>key;
    
    // search(arr, 11, key);
    cout<<search(arr,11,key);
}
